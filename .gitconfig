[user]
    name = Laurence Foley
    email = lrncfly@gmail.com
[alias]
    st = status
[color]
    ui = auto
[commit]
	template = ~/.stCommitMsg
[core]
	excludesfile = ~/.gitignore_global
[include]
    path = ~/.gitlocal
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
[push]
    default = simple
